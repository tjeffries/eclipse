import java.io.FileInputStream;
import java.util.Scanner;

/* Author: Thomas Jeffries
 * Main class for Point Location
 * */
public class PLocate {
	
	public static void main(String[] args) {
		try{
			Scanner s = new Scanner(new FileInputStream("C:/Users/thomas/workspace/Project3/bin/in.txt"));
			
			if(s.hasNext()){
				for(int i=0; i<s.nextInt(); i++){
					for(int j=0; j<4; j++){
						System.out.printf("%f ", s.nextDouble());
					}
					s.nextLine();
					System.out.println();
				}
				
			}
			
			s.close();
		} catch (Exception e){
			e.printStackTrace();
		}
	}
}
